### z score ###
f_z <- function(x){
m <- base::mean(x,na.rm=TRUE)
sd <- sd(x,na.rm=TRUE)
output <- (x-m)/sd
return(output)
}
# names(df_resultados)
df_resultados.z <- as.data.frame(apply(df_resultados[c(6:12,19)],2,f_z))
